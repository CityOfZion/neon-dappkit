{
  "all": true,
  "check-coverage": true,
  "exclude": ["**/dist/*", "**/*.spec.ts"],
  "extends": "@istanbuljs/nyc-config-typescript",
  "include": ["src/**/*.ts"],
  "report-dir": "./reporter",
  "reporter": ["html", "json", "json-summary", "text"],
  "watermarks": {
    "lines": [0, 80],
    "functions": [0, 80],
    "branches": [0, 80],
    "statements": [0, 80]
  }
}
